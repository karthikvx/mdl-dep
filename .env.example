# Copy this to .env and fill in your values

# Service Configuration
SERVICE_TYPE=orchestrator
PORT=5000
FLASK_DEBUG=false
HOST=0.0.0.0

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
MODEL_BUCKET=mortgage-ml-models

# Service URLs (for orchestrator)
PRICING_SERVICE_URL=http://localhost:5001
PREDICTION_SERVICE_URL=http://localhost:5002

# Database Configuration (if using external DB)
DATABASE_URL=postgresql://user:pass@localhost:5432/mortgage_db

# Monitoring and Alerting
ALERT_EMAIL=alerts@yourcompany.com
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# Application Settings
APP_VERSION=1.0.0
LOG_LEVEL=INFO
